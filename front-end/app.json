{
  "expo": {
    "name": "VidaPet",
    "slug": "VidaPet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "VidaPet",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.viniciuszancanari.VidaPet"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "expo-router": {
        "appRoot": "src/app"
      },
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2e66cc67-d034-45c7-8db8-c772e05e13e1"
      }
    }
  }
}
